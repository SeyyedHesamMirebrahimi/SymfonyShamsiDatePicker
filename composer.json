    "homepage": "http://hmirebrahimi.ir",
    "license": "MIT",
    "authors": [
        {
            "name": "Seyyed Hesam Mirebrahimi",
            "homepage": "https://github.com/SeyyedHesamMirebrahimi"
        }
    ],
  "require" : {
    "php": "^5.5.9 || ^7.0"
  },
  "autoload" : {
    "exclude-from-classmap": ["/Tests/"],
    "psr-4": {
      "Hesam\\ShamsiDate\\" : ""
    }
